# Telegram Bot Configuration
TELEGRAM_BOT_API=your_bot_token_here
GEMINI_API=your_gemini_api_key_here

# Admin's Telegram chat ID for receiving channel owner form submissions
# To get your chat ID, message @userinfobot on Telegram
# For groups/channels, use the group/channel ID (starts with -100)
ADMIN_CHAT_ID=your_admin_chat_id_here

# Admin's Telegram chat ID for backup restoration commands
# Use your personal chat ID here (your user ID as a number)
# This should be your personal user ID, NOT a group/channel ID
ADMIN_CHAT_ID_BACKUP=your_personal_user_id_here
